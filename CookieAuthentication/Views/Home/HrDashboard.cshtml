@{
    ViewData["Title"] = "HR Dashboard";
}
@model IEnumerable<CookieAuthentication.DTO.BookDTO>

<h1>HR Dashboard</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <td>Title</td>
            <td>Description</td>
            <td>Rate</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.ToList())
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.Rate</td>
            </tr>
        }
        
    </tbody>
</table>


