@{
    ViewData["Title"] = "HR Dashboard";
}
@model IEnumerable<CookieAuthentication.DTO.WeatherDTO>


<h1 class="">HR Dashboard</h1>
<hr />

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <td >Date</td>
            <td>TemperatureC</td>
            <td>TemperatureF</td>
            <td>Summary</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.ToList())
        {
            <tr>
                <td>@item.Date</td>
                <td>@item.TemperatureC</td>
                <td>@item.TemperatureF</td>
                <td>@item.Summary</td>
            </tr>
        }
        
    </tbody>
</table>


